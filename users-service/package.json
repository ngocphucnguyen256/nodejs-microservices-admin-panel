{
  "name": "user-service",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "watch": "ts-node-dev --respawn --poll index.dev.ts",
    "db:migrate": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/db/data-source.ts",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d src/db/data-source.ts -o src/db/migrations",
    "db:migrate:undo": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/db/data-source.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/config": "~0.0.38",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash.omit": "^4.5.9",
    "@types/node": "^20.11.21",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "module-alias": "^2.2.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.11",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "lodash.omit": "^4.5.0",
    "mysql2": "^3.9.1",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.1",
    "uuid": "^9.0.1"
  }
}
