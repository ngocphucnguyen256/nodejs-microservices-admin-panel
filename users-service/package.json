  },
  "devDependencies": {
    "@types/config": "~0.0.38",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.21",
    "module-alias": "^2.2.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "config": "^3.3.11",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mysql2": "^3.9.1",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.1"
  }
}
